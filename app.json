{
  "expo": {
    "name": "The Colony",
    "slug": "the-colony",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/lb.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoLogin2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAA91C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.abdullah_naveed.thecolony"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow The Colony to access your photos",
          "cameraPermission": "Allow The Colony to access your Camera"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d3f35e2e-b54f-4292-85bb-7f000a3de62c"
      }
    }
  }
}

{
  "expo": {
    "name": "The Colony",
    "slug": "the-colony",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/lb.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoLogin2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAA91C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.waqarmughal005.thecolony"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.waqarmughal005.thecolony"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow The Colony to access your photos",
          "cameraPermission": "Allow The Colony to access your Camera"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow The Colony to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1c87c96f-155c-4857-a182-45051abeff28"
      }
    }
  }
}

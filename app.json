{
  "expo": {
    "name": "The Colony Solutions",
    "slug": "the-colony",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/lb.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoLogin2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAA91C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.waqarmughal005.thecolony",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.waqarmughal005.thecolony",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow The Colony to access your photos",
          "cameraPermission": "Allow The Colony to access your Camera"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1873c7ad-3dff-4dcc-9d79-3c1eee1274fa"
      }
    }
  }
}
